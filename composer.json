{
    "name": "flexcoders/omnipay-govpaynet",
    "type": "library",
    "description": "GovPayNet driver for the Omnipay payment processing library",
    "keywords": [
        "govpaynet",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "flexcoders"
    ],
    "homepage": "https://github.com/flexcoders/omnipay-govpaynet",
    "license": "MIT",
    "authors": [
        {
            "name": "FlexCoders Ltd",
            "homepage": "http://flexcoders.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\GovPayNet\\" : "src/"
        },
        "classmap": {
            "FlexCoders\\GovPayNet\\" : "soap/"
        }
    },
    "require": {
        "omnipay/common": "~2.0"
    },
    "require-dev": {
        "omnipay/tests": "~2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
